{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["tsconfig.json"]
    },
    "plugins": [
        "react",
        "import"
    ],
    "rules": {
      "quotes": ["error", "double"],
      "@typescript-eslint/quotes": ["error", "double"]
    }
}
